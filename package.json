{
  "name": "chaturbapi",
  "version": "1.0.0",
  "description": "Library to interact with the Chaturbate API's",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc --declaration --emitDeclarationOnly --outFile dist/index.d.ts",
    "typecheck": "tsc --noEmit",
    "test": "npx tsx examples/test.ts"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.2"
  },
  "keywords": [
    "chaturbate",
    "api",
    "affiliate",
    "statistics"
  ],
  "author": "@nikitacontreras",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikitacontreras/chaturbapi.git"
  },
  "bugs": {
    "url": "https://github.com/nikitacontreras/chaturbapi/issues"
  },
  "homepage": "https://github.com/nikitacontreras/chaturbapi#readme"
}